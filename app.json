{
  "expo": {
    "name": "CozyMeals",
    "slug": "CozyMeals",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Images/CozyMealsLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#A9744C"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Images/CozyMealsLogo.png",
        "backgroundColor": "#A9744C"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
